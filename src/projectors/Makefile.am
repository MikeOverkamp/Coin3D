if HACKING_COMPACT_BUILD
BuildSources = $(LinkHackSources)
else
BuildSources = $(RegularSources)
endif

if BUILD_WITH_MSVC
noinst_LIBRARIES = projectors.lst
else
if HACKING_DYNAMIC_MODULES
lib_LTLIBRARIES = libprojectors@SUFFIX@LINKHACK.la
else
noinst_LTLIBRARIES = libprojectors.la
endif
endif

# Remove any linklibraries detected by configure, as they are not
# needed in the submodules -- only in the toplevel libCoin build.
LIBS = 

INCLUDES = -I$(top_srcdir)/include -I$(top_builddir)/src/extensions

libprojectors_la_SOURCES = $(BuildSources)
EXTRA_libprojectors_la_SOURCES = \
	$(PublicHeaders) $(PrivateHeaders) $(RegularSources) $(LinkHackSources)

libprojectors@SUFFIX@LINKHACK_la_SOURCES = $(BuildSources)
EXTRA_libprojectors@SUFFIX@LINKHACK_la_SOURCES = \
	$(PublicHeaders) $(PrivateHeaders) $(RegularSources) $(LinkHackSources)

libprojectorsincdir = $(includedir)/Inventor/projectors
libprojectorsinc_HEADERS = $(PublicHeaders)

projectors_lst_SOURCES = $(BuildSources)
EXTRA_projectors_lst_SOURCES = \
	$(PublicHeaders) $(PrivateHeaders) $(RegularSources) $(LinkHackSources)

# this rule causes a make warning, so we remove it when it is not in use
projectors.lst: Makefile $(projectors_lst_OBJECTS)
	@echo "Linking projectors.lst..."; \
	rm -f $@; \
	for i in $(projectors_lst_OBJECTS); do echo $$i >>$@; done

install-libprojectorsincHEADERS: $(libprojectorsinc_HEADERS)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(libprojectorsincdir)
	@list='$(libprojectorsinc_HEADERS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  src="$$d$$p"; dst="$(DESTDIR)$(libprojectorsincdir)/$$f"; \
	  if cmp -s "$$src" "$$dst"; then :; else \
	    echo " $(INSTALL_HEADER) $$src $$dst"; \
	    $(INSTALL_HEADER) "$$src" "$$dst"; \
	  fi \
	done
	@list='$(ObsoletedHeaders)'; for f in $$list; do \
	  file="$(DESTDIR)$(libprojectorsincdir)/$$f"; \
	  if test -x "$$file"; then \
	    echo " rm -f $$file"; \
	    rm -f "$$file"; \
	  fi \
	done

